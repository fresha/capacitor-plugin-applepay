{
  "name": "@ryancrummy-dept/capacitor-plugin-applepay",
  "version": "5.0.0",
  "description": "Apple Pay plugin for Capacitor",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "type": "module",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "dist/",
    "ios/Plugin/",
    "FreshaCapacitorPluginApplepay.podspec"
  ],
  "author": "Fresha",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryancrummy-dept/capacitor-plugin-applepay.git"
  },
  "bugs": {
    "url": "https://github.com/ryancrummy-dept/capacitor-plugin-applepay/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --fix --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api ApplePayPlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@capacitor/core": "^5.0.0",
    "@capacitor/docgen": "^0.2.1",
    "@capacitor/ios": "^5.0.0",
    "@ionic/swiftlint-config": "^1.1.2",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "~3.0.0",
    "prettier-plugin-java": "~2.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.3",
    "swiftlint": "^1.0.2",
    "typescript": "~5.1.6"
  },
  "peerDependencies": {
    "@capacitor/core": "^5.0.0"
  },
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2020,
      "parser": "@typescript-eslint/parser"
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ]
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
